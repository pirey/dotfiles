; http://editorconfig.org/

root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true


[*.{c,h}]
indent_style             = tab
tab_width                = 4

[*.{yml,json,html,js}]
indent_size              = 2
